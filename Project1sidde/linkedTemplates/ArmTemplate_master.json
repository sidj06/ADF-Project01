{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Project1sidde"
		},
		"Linked_service_blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Linked_service_blob'"
		},
		"linked_service_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linked_service_adls'"
		},
		"linked_service_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://projectde01.dfs.core.windows.net/"
		},
		"Storage event trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/63a10f88-4534-476a-b0ff-a850246ac2af/resourceGroups/project001/providers/Microsoft.Storage/storageAccounts/projectde01blob"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Project1sidde_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Linked_service_blob_connectionString": {
						"value": "[parameters('Linked_service_blob_connectionString')]"
					},
					"linked_service_adls_accountKey": {
						"value": "[parameters('linked_service_adls_accountKey')]"
					},
					"linked_service_adls_properties_typeProperties_url": {
						"value": "[parameters('linked_service_adls_properties_typeProperties_url')]"
					},
					"Storage event trigger_properties_typeProperties_scope": {
						"value": "[parameters('Storage event trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}